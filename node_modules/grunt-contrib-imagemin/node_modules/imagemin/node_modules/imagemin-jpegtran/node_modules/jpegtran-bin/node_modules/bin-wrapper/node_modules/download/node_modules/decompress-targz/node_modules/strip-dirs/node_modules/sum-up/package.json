{
  "name": "sum-up",
  "version": "1.0.1",
  "description": "Summarize package information",
  "repository": "shinnn/sum-up",
  "homepage": "https://github.com/shinnn/sum-up",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js --color && node test.js --no-color",
    "coverage": "istanbul cover test.js -- --color",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/sum-up/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "summerize",
    "summary",
    "brief",
    "outline",
    "info",
    "information",
    "description",
    "data",
    "package",
    "color",
    "cli-friendly"
  ],
  "dependencies": {
    "chalk": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^0.15.0",
    "istanbul": "^0.3.6",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
